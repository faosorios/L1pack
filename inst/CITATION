citHeader("To cite L1pack in publications use:")

bibentry(bibtype = "Manual",
         title = "Routines for L1 estimation",
         author = c(as.person("F. Osorio"),
                    as.person("T. Wolodzko")),
         year = "2026",
         note = "R package version 0.62",
         url  = "https://github.com/faosorios/L1pack",
         textVersion =
          paste("Osorio, F., Wolodzko, T. (2026).",
                "Routines for L1 estimation.",
                "R package version 0.62.", "URL: https://github.com/faosorios/L1pack")
)
